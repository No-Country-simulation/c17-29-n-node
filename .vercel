{
  "version": 2,
  "builds": [
    {
      "src": "server/src/app.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "server/src/**/**/*.js"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/src/app.js"
    }
  ]
}
